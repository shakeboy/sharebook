.other,page{
  width: 100%;
  height: 100%;
  background-color:#fff;
}
.other_top{
  width:100%;
  height:140px;
  border: 4px solid rgb(21, 136, 126);
  margin-left: 2px;
  margin-right: 10px;
  border-radius: 10px;
  margin-top: 0px;
  display: flex;
  flex-direction: row;
  padding-top: 10px;
  padding-left: 10px;
}
.img,.img>image{
  width:120px;
  height:120px;
  border-radius: 60px;
}
.name_sig{
  width: fit-content;
  height: 140px;
  display: flex;
  flex-direction: column;
  padding-left: 10px;
}
#name{
  margin-top: 40px;
  /* border: solid 1px blue; */
  font-size: 20px;
}
#sig{
  margin-top: 50px;
  border: solid 1px darksalmon;
  border-radius: 4px;
  margin-right: 0;
  font-size: 10px;
  overflow: hidden;
}
.article{
  border: 4px solid rgb(21, 136, 126);
  margin-left: 2px;
  margin-right: 10px;
  border-radius: 10px;
  margin-top: 10px;
  width: 100%;
  height:200px;
  overflow: hidden;
}
.bottom{
  bottom: 0;
  position: fixed;
  width: 100%;
  height: 40px;
  background-color: yellow;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.but{
  width: 50%;
  background-color: #fff;
  line-height: 40px;
}